      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.02 0.02"/>
      </geometry>
    </collision>
  </link>

  <!-- RIGHT AILERON CHAIN -->
  <joint name="fixed_joint_right_aileron_mount" type="fixed">
    <parent link="fuselage"/>
    <child  link="fuselage_right_aileron_0"/>
