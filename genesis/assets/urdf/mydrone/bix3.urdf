<robot name="drone">
  <material name="grey">
    <color rgba="0.7 0.7 0.7 1"/>
  </material>
  <material name="red">
    <color rgba="0.7 0 0 1"/>
  </material>
  <material name="black">
    <color rgba="0.2 0.2 0.2 1"/>
  </material>

  <link name="root_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>

  <joint name="fixed_joint_base_fuselage" type="fixed">
    <parent link="root_link"/>
    <child link="fuselage"/>
    <origin rpy="3.14159 0 0" xyz="0 0 0"/>
  </joint>

  <link name="fuselage">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.32 0 -0.02"/>
      <mass value="0.875"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.096"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://meshes/fixed_wing_fuselage.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.7 0.3 0.3"/>
      </geometry>
    </collision>
  </link>

  <!-- AERO FRAME -->
  <joint name="fixed_joint_aero_frame_fuselage" type="fixed">
    <parent link="fuselage"/>
    <child  link="aero_frame_fuselage"/>
    <origin rpy="0 3.14159 0" xyz="-0.32 0 -0.02"/>
  </joint>
  <link name="aero_frame_fuselage">
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- LEFT AILERON CHAIN -->
  <joint name="fixed_joint_left_aileron_mount" type="fixed">
    <parent link="fuselage"/>
    <child  link="fuselage_left_aileron_0"/>
    <origin rpy="0 0 0" xyz="-0.364 -0.275 -0.042"/>
  </joint>
  <link name="fuselage_left_aileron_0">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.018"/>
      <inertia ixx="2.13e-06" ixy="0" ixz="0" iyy="2.13e-06" iyz="0" izz="2.13e-06"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </collision>
  </link>
  <joint name="joint_left_aileron" type="revolute">
    <parent link="fuselage_left_aileron_0"/>
    <child  link="left_aileron_wing"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis  xyz="0 1 0"/>
    <limit lower="-0.3491" upper="0.3491" effort="5.0" velocity="5.4"/>
    <dynamics damping="1" friction="0"/>
  </joint>
  <link name="left_aileron_wing">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.02 0 0"/>
      <mass value="0.013"/>
      <inertia ixx="9e-05" ixy="0" ixz="0" iyy="2e-06" iyz="0" izz="9e-05"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://meshes/fixed_wing_left_aileron.stl" scale="0.001 0.001 0.001"/>
