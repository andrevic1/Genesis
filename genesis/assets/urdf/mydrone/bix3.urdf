    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://meshes/fixed_wing_right_aileron.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.02 0.02"/>
      </geometry>
    </collision>
  </link>

  <!-- Rudder assembly -->
  <joint name="fixed_joint_rudder_wing" type="fixed">
    <parent link="fuselage"/>
    <child link="fuselage_rudder_0"/>
    <origin rpy="0 0 0" xyz="-0.82 0 0.01"/>
  </joint>
  <link name="fuselage_rudder_0">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.018"/>
      <inertia ixx="2.13e-06" ixy="0" ixz="0" iyy="2.13e-06" iyz="0" izz="2.13e-06"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
