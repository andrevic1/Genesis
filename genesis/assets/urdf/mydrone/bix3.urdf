  </joint>
  <link name="prop_frame_fuselage">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.04"/>
      <inertia ixx="3.7625e-05" ixy="0" ixz="0" iyy="3.7625e-05" iyz="0" izz="7.5e-05"/>
